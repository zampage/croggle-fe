{% extends 'layout/default.html.twig' %}

{% block pageTitle %}{{ page.title }}{% endblock %}

{% block content %}
    <section>
        {{ page.content }}
    </section>
    {% for module in page.collection() %}
        <section>
            <h2>{{module.title}}</h2>
            {{module.content}}
        </section>
    {% endfor %}
{% endblock %}
